$result="";
    foreach($this->participants as $participants){
        if(trim($participants['lat'])!="" && trim($participants['long'])!=""){
           $result[]=$participants['lat']."#".$participants['long']."#".$participants['first_name'].$participants['last_name']."<br/>".$participants['address'];
        }
    }
echo json_encode($result);