    function GetPrint() {
        paginate = false;
        drawTable()
        window.print();
    }
    jQuery(document).bind("keyup keydown", function (e) {
        if (e.ctrlKey && e.keyCode == 80) {
            paginate = false;
            drawTable()
        }
